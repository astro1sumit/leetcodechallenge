import java.util.*;
class Solution {
    public String largestOddNumber(String num) {
        int n=num.length()-1;
        while(n>=0){
            int pp=num.charAt(n);
            if(pp%2!=0){
                return num.substring(0,n+1);
            }
            else{
                n--;
            }
        }
        return "";
    }
}
